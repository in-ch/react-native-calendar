{
  "name": "reactnativecalendar",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lintUpdate": "npx eslint ./src/**/*.tsx --fix",
    "format": "prettier --check ./src",
    "formatUpdate":"npx prettier --write ./src",
    "start": "react-native start",
    "test": "jest",
    "pod": "cd ios && arch -x86_64 pod install",
    "androidTcp": "adb -s emulator-5554 reverse tcp:3000 tcp:3000",
    "iosPhone": "react-native run-ios --device '인치의 iPhone'",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react": "^18.0.5",
    "@types/react-native": "^0.67.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
